@model IEnumerable<Auction.Models.Tickets>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumEarlyTickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumDoorTickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostEarlyTickets)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostDoorTickets)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumEarlyTickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumDoorTickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostEarlyTickets)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostDoorTickets)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
